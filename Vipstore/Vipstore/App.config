<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <appSettings>
    <!--FailIfMissing=false表示数据库不存在时就会自动创建-->
    <add key="DbSQLite" value="data source=|DataDirectory|VIPStore.db;Pooling=true;FailIfMissing=false"/>
  </appSettings>
</configuration>